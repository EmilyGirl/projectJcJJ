	.wapper {
		width: 100%;
		height: 100%;
		background: #eaeaea;
		position: absolute;
	}
	.clearfix::after {
		content: "";
		display: block;
		height: 0;
		clear: both;
		visibility: hidden
	}
	.slogan{
		height:4rem;
		line-height: 4rem;
		background-color: #FFF0E1;
		color: #FF6000 ;
		text-align: center;
		font-size: 1.2rem;
		span {
			display: inline-block;
			width: 1.4rem;
			// padding-right: 1.2rem;
			position: absolute;
			right: 1rem;
			line-height: 4rem;
		}

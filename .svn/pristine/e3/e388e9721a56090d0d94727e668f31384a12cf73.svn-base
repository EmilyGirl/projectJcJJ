.homeTabs .homeTabs{
	width: 100%;
    height: 100vh;
    background: #eaeaea;
    position: relative;
    overflow-x: hidden;
    overflow-y: scroll;
}

.homeTabs .tab {
	width: 100%;
    padding: 1.03rem 0 0.78rem 0;
    background: #fff;
    position: absolute;
    font-size: 1.2rem;
    z-index: 1000;
    bottom: 0;
    border-top: .1rem solid #E1E1E1;
}

.homeTabs .tab li {
    width: 50%;
    float: left;
    font-size: .9em;
    text-align: center;
}

.homeTabs .tab li a {
    color: #999;
}

.homeTabs .tab li.cur a {
    color: #333;
}

.homeTabs .tab .active.nav-item:nth-child(1) span {
    width: 1.845rem;
    height: 1.74rem;
    display: block;
    background: url(../../img/wechat/shouye/cmoney.png) no-repeat;
    background-size: 100% 100%;
    margin: 0 auto .375rem auto;
}

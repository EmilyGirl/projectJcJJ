.product-detail {
    position: absolute;
    width: 100%;
    min-height: 100%;
    // .page1 
    .bang-mask {
        position: absolute;
        width: 100%;
        height: 100%;
        // background: rgba(0, 0, 0, 0.5);
        background: transparent;
        z-index: 10000;
    }
    .bang-content {
        width: 32.7rem;
        height: 17rem;
        background: #fff;
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        border-radius: 1.25rem;
    }
    .bang-content p {
        text-align: center;
    }
    .bang-title {
        font-size: 1.8rem;
        margin: 1.5rem 0;
        color: #000;
    }
    .bang-common {
        font-size: 1.6rem;
        color: #666;
        line-height: 2.3rem;
        margin-bottom: 1.5rem;
    }
    .bangBtn {
        width: 90%;
        margin: 0 auto;
    }
    .bangBtn a,
    .bangBtn span {
        display: inline-block;
        width: 13.2rem;
        height: 4rem;
        text-align: center;
        line-height: 4rem;
        font-size: 1.6rem;
        color: #fff;
        border: 1px solid@mainColor;
        border-radius: 0.4rem;
    }
    /*.bangBtn a span { display: inline-block;width: 100%;height: 100%; }*/
    .bangBtn .firstBtn {
        color: @mainColor;
    }
    .secondBtn {
        background: @mainColor;
    }
    .product-header {
        background-image: @gradients;
        box-shadow: 0 6px 6px 0 rgba(255, 96, 96, 0.16);
        height: 15.2rem;
        text-align: center;
        background-size: cover;
    }
    .product-header .title-top {
        font-size: 1.4rem;
        line-height: 2rem;
        color: #fff;
        text-align: center;
        // margin: 1rem 0; 
        padding-top: 1rem;
        img {
            width: 1.36rem;
            ;
        }
    }
    .title {
        text-align: left;
        padding: 1.6rem 0 0 1.5rem;
        font-size: 1.4rem;
        color: #fff;
        line-height: 2rem;
    }
    .interest {
        padding-top: 3rem;
        text-align: center;
    }
    .yuan {
        font-size: 5.3rem;
        line-height: 6rem;
        color: #fff;
    }
    .yuan-mum {
        font-size: 3rem;
        color: #fff;
    }
    .jiali {
        font-size: 3rem;
        color: #fff;
    }
    .jiali-num {
        font-size: 1.5rem;
        color: #fff;
    }
    .info-title {
        text-align: center;
        margin: 1rem 0;
    }
    .info-title span {
        display: inline-block;
        padding: 0.5rem 0.9rem;
        border: 1px solid #fff;
        border-radius: 0.4rem;
        font-size: 1.2rem;
        color: #fff;
        margin-left: 1rem;
    }
    .detail {
        font-size: 1.6rem;
        color: #fff;
        line-height: 2.3rem;
        padding-bottom: 3.5rem;
    }
    .progress {
        position: relative;
        //    height: 5rem;
        background: @mainColor;
        text-align: center;
        margin-bottom: 1.2rem;
    }
    .schedule {
        font-size: 1.1rem;
        color: @mainColor;
        text-align: left;
        // margin-bottom: 0.9rem;
        display: inline-block;
    }
    .schedule1 {
        width: 100%;
    }
    .schedule .progress-width {
        display: inline-block;
        height: 0.2rem;
        width: 100%;
        margin-top: -0.2rem;
    }
    .progress_text {
        position: absolute;
        text-align: right;
        top: -1.9rem;
        font-size: 1.1rem;
        color: #fff;
    }
    .schedule span var {
        display: inline-block;
        background: @progressGradients;
        height: 0.2rem;
        vertical-align: text-top;
    }
    .progress-info {
        width: 34.4rem;
        margin: 0 auto;
        padding-bottom: 0.6rem;
    }
    .progress-info div {
        font-size: 1.2rem;
        color: #fff;
        line-height: 1.7rem;
    }
    .calculator {
        background: #fff;
    }
    .calculator-content {
        margin-left: 1.7rem;
    }
    .border {
        // border-top: 1px solid #E1E1E1;
    }
    .calculator-common {
        border-bottom: 1px solid #E0E0E0;
        height: 4.5rem;
        margin-right: 1.5rem;
    }
    .calculator-common span,
    .calculator-common input {
        font-size: 1.6rem;
        line-height: 4.4rem;
    }
    .calculator-common input {
        margin-left: 1.5rem;
        text-align: right;
        // margin-right: 1.5rem;
        vertical-align: baseline;
        float: right;
        //          margin-left: 5rem;
        //          
    }
    .calculator-common .line {
        width: 0.1rem;
        height: 1.6rem;
        color: @mainColor;
        border: 0.05rem solid@mainColor;
        margin-right: 1rem;
    }
    .calculator-common .all {
        margin-right: 1.5rem;
        padding-left: 1rem;
        color: @mainColor;
        //      border-left: 1px solid@mainColor;;
    }
    .calculator-common var,
    .calculator-common p {
        font-size: 1.6rem;
        line-height: 4.5rem;
    }
    .calculator-common var {
        color: #6A6A6A;
    }
    .calculator-common p {
        color: @mainColor;
        // margin-right: 1.5rem;
    }
    .algorithm {
        margin-top: 1rem;
        padding-left: 1.5rem;
        background-color: #fff;
    }
    .algorithm p {
        font-size: 1.2rem;
        color: #9A9A9A;
        line-height: 2rem;
    }
    .prodDesc {
        padding: .6rem 0;
    }
    .btn {
        position: fixed;
        bottom: 0;
        left: 0;
        width: 100%;
        line-height: 5rem;
        background: #fff;
        font-size: 1.6rem;
        color: #fff;
        z-index: 100;
        border-top: 1px solid #efe5e5;
        .botTitle {
            display: inline-block;
            height: 5rem;
            width: 70%;
            text-align: center;
            color: #666666;
        }
        .btnBot {
            position: fixed;
            bottom: 0;
            right: 0;
            //          right: 0 !important;
            display: inline-block;
            height: 5rem;
            width: 30%;
            background: rgb(221, 221, 221);
        }
        .on {
            background: @mainColor;
            color: #fff;
        }
    }
    .finish {
        background: #ccc;
    }
    .infoTitle {
        padding: 0 2.2rem;
        padding-top: 1.8rem;
        line-height: 2rem;
        display: flex;
        justify-content: space-around;
        color: #fff;
        font-size: 1.3rem;
    }
    .algorithm p.productIntroduction {
        font-size: 1.4rem;
        color: #3A3A3A;
        // line-height: 3.6rem;
        height: 4rem;
        display: flex;
        align-items: center;
        border-bottom: 1px solid #E0E0E0;
        // padding-bottom: .5rem;
    }
    .algorithm p span.blackLine {
        display: inline-block;
        width: .2rem;
        margin-right: 0.9rem;
        height: 1.6rem;
        background: #3291FF;
    }
    .resdata {
        color: #202020;
        // display: inline-block;
        padding-left: 1rem;
    }
    .prompting {
        padding-top: 1.5rem;
        color: #A0A0A0;
        text-align: center;
        font-size: 1.2rem;
        line-height: 2.4rem;
    }
    .more {
        width: 100%;
        text-align: center;
        padding: 1rem 0   7rem  0 ;
        font-size: 1.2rem;
        color: #A0A0A0;
        animation-name: bounce;
        transform-origin: center bottom;
        animation-duration: 2.5s;
        animation-fill-mode: both;
        animation-iteration-count: infinite;
//      background-color: rgba(0,0,0,.5);
//      padding: 1rem;
//      width: 20rem;
//      left: 50%;
//      margin-left: -10rem;
//      color: #FFFFFF;
    }
    @keyframes bounce {
        0%,
        20%,
        53%,
        80%,
        to {
            animation-timing-function: ease-in-out;
            transform: translateZ(0)
        }
        40%,
        43% {
            animation-timing-function: ease-in-out;
            transform: translate3d(0, -15px, 0)
        }
        70% {
            animation-timing-function: ease-in-out;
            transform: translate3d(0, -5px, 0)
        }
        90% {
            animation-timing-function: ease-in-out;
            
            transform: translate3d(0, -0px, 0)
        }
    }
    .more img {
        width: 1.2rem;
        margin-right: .5rem;
    }
    .page1 {
        // position: fixed;
        width: 100%;
        position: absolute;
        height: 90%;
        top: 0;
        left: 0;
        padding-bottom: 8rem;
    }
    .ComWarp,
    .page1 {
        position: absolute;
        top: 0;
        width: 100%;
    }
    .product_desc {
        display: flex;
        justify-content: space-around;
        color: #fff;
        font-size: 1.2rem;
        margin-top: -.8rem;
    }
    .desc_item {
        display: flex;
        flex-direction: column;
        line-height: 2rem;
        height: 4.4rem;
    }
    .desc_num {
        font-size: 1.6rem;
    }
    .tip_detail {
        width: 38%;
        height: 3.2rem;
        line-height: 3.2rem;
        opacity: 0.8;
        font-family: PingFang-SC-Medium;
        font-size: 1.3rem;
        color: #FF6000;
        text-align: center;
        background-size: 100% 100%;
        background-repeat: no-repeat;
        // background-attachment:fixed;
        // background-position:center;
        margin: 0 auto;
        background-image: url('./../../img/wechat/product/tipsBG.png')
    }
}

.slideInDown {
    animation-name: slideInDown;
    animation-duration: 0.8s;
    animation-fill-mode: both
}

.slideInUp {
    animation-name: slideInUp;
    animation-duration: .8s;
    animation-fill-mode: both
}

.slideOutDown {
    animation-name: slideOutDown;
    animation-duration: .8s;
    animation-fill-mode: both
}

.slideOutUp {
    animation-name: slideOutUp;
    animation-duration: .8s;
    animation-fill-mode: both;
}

@keyframes slideOutUp {
    0% {
        transform: translateZ(0)
    }
    to {
        visibility: hidden;
        transform: translate3d(0, -100%, 0)
    }
}

@keyframes slideOutDown {
    0% {
        transform: translateZ(0)
    }
    to {
        visibility: hidden;
        transform: translate3d(0, 100%, 0)
    }
}

@keyframes slideInUp {
    0% {
        transform: translate3d(0, 100%, 0);
    }
    to {
        transform: translateZ(0)
    }
}

@keyframes slideInDown {
    0% {
        transform: translate3d(0, -100%, 0);
    }
    to {
        transform: translateZ(0)
    }
}
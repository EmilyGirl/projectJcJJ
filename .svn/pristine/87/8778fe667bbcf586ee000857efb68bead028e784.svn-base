.money_wrapped *{
    margin: 0;
    padding: 0;
    box-sizing: border-box
}
.money_wrapped .clear{
    clear: both;
}
.money_wrapped .wrapped_title{
    width: 100%;
    height: 4.5rem;
    background: #FFFFFF;
    border-top: 1px solid #EBEBEB;
    border-bottom:1px solid #EBEBEB;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
}
.money_wrapped .wrapped_box{
    margin-top: 6rem;
}
.money_wrapped .wrapped_title li{
    float: left;
    width: 25%;
    height: 4.5rem;
    text-align: center;
    line-height: 4.5rem;
    font-size: 1.5rem;
    color: #000000;
}
.money_wrapped .lijin{
    padding-top: 3.5rem!important
}
.money_wrapped .wrap_li{
    color: @mainColor!important;
    border-bottom: 0.2rem solid @mainColor;
}
.money_wrapped .wrapped_content{
    width: 34.5rem;
    min-height: 10rem;
    margin: 1.6rem 1.5rem;
    position: relative;
}
.money_wrapped .wrapped_content .content_img{
    position: absolute;
    width: 34.5rem;
    height: 10rem;
    left: 0;
    top: 0
}
.money_wrapped .content_left{
    width: 10.5rem;
    height: 10rem;
    position: absolute;
    /* left: 2.8rem;
    top: 2.15rem; */
    left: 0;
    top: 0;
    color: #fff;
    z-index: 100;
    text-align: center;
    padding: 2.15rem 1rem;
    line-height: 2.5rem;
}
.money_wrapped .content_right{
    width: 24rem;
    min-height: 10rem;
    position: relative;
    left: 10.5rem;
    top: 0;
    z-index: 100
}
.money_wrapped .left_one1{
    font-size: 1.3rem;
    font-family: Helvetica;
}
.money_wrapped .left_one{
    font-size: 3rem;
    font-family: Helvetica;
}
.money_wrapped .content_noob{
   width: 7.85rem;
   height: 1.85rem;
   color: #888888;
   font-size:1.3rem;
   top:  1.8rem;
   left: 1rem;
   position: relative;
}
.money_wrapped .content_time{
    color: #888888;
    margin-left: 1rem; 
    top: 3.05rem; 
    left: 0rem;
    font-size: 1.3rem;
    position: relative;
}
.content_source{
    position: relative;
    top: 1rem;
    left: 1rem;
}
.money_wrapped .coupon{
	border-radius: 0.25rem;
    padding: 0 0.4rem;
    height: 1.8rem;
    color: #fff;
    font-size: 1.3rem;
    text-align: center;
    line-height: 1.8rem;
    float: left
}
.money_wrapped .coupon3{
    background: @mainColor;
}
.money_wrapped .coupon2{
    background: #FF9C0F;
}
.money_wrapped .coupon1{
    background:  #F4583F;
}
.money_wrapped .coupon0{
    background: #c2c2c2;
}
.money_wrapped .coupon4{
    background: #6C3FF4;
}
.money_wrapped .coupon_source{
    font-size: 1.4rem;
    color: #000000;
    /* margin: 0.3rem 0 0 1rem; */
    margin-top: 0.3rem;
    float: left;
}
.money_wrapped .coupon_source1{
   margin-left: 1rem;
}
.money_wrapped .guoqi{
    width: 4.33rem;
    height: 3.8rem;
    position: absolute;
    right: 0;
    top: 6.2rem;
    z-index: 102;
}
.money_wrapped .rule{
    font-size: 1.3rem;
    color: #999999;
    width: 6.95rem;
    height: 1.85rem;
    margin: 1rem auto 6rem;
}
.money_wrapped .buy{
    width: 37.5rem;
    height: 5rem;
    background:  @mainColor;
    font-size: 1.6rem;
    color: #fff;
    text-align: center;
    line-height: 5rem;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 9999;
}
.money_wrapped .card-null { margin-top: 2.7rem;text-align: center; }
.money_wrapped .card-null img{ width: 17.9rem;height: 13.7rem;margin-bottom: 1.2rem; }
.money_wrapped .card-null p { font-size: 1.8rem;line-height: 2.5rem;color: #000; }
.money_wrapped .card-null a { width: 34.4rem;height: 4.5rem;background: @mainColor;line-height: 4.5rem;display: inline-block;text-align: center;color: #fff;font-size: 1.6rem;margin-top: 2.7rem;border-radius: 0.4rem; }


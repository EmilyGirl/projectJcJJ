    }
    .calculator-common .all {
        margin-right: 1.5rem;
        padding-left: 1rem;
        color: #2772FF;
        //      border-left: 1px solid #2772FF;;
    }
    .calculator-common var,
    .calculator-common p {
        font-size: 1.2rem;
        line-height: 4.5rem;
    }
    .calculator-common var {
        color: #6A6A6A;
    }
    .calculator-common p {
        color: #FF8200;
        margin-right: 1.5rem;
    }
    .algorithm {
        margin-top:1rem; 
        padding: 1rem 1.5rem .5rem;
        background-color: #fff;
    }
    .algorithm p {
        font-size: 1.2rem;
        color: #9A9A9A;
        line-height: 2rem;
    }
    .btn {
        position: fixed;
        bottom: 0;
        left: 0;
        width: 100%;  
        line-height: 5rem;
        background: #fff;
        font-size: 1.6rem;
        color: #fff;
        z-index: 100;
        .botTitle {
            display: inline-block;
             height: 5rem;
             width: 70%;
            text-align: center;
            color: #666666;
        }
        .btnBot {
            position: fixed;
             bottom: 0;
             right: 0;
//          right: 0 !important;
            display: inline-block;
             height: 5rem;
             width: 30%;
             background: rgb(221, 221, 221);
        }
        .on{
            
            background: #2772FF;
            color: #fff;
        }
    }
    .finish {
        background: #ccc;
    }
    .infoTitle{
        padding: 0 2.2rem;
        padding-top: 1.8rem;
        line-height: 2rem;
        display: flex;
        justify-content: space-around;
        color: #fff;
        font-size: 1.3rem;
    }
    .algorithm  p.productIntroduction{
        font-size: 1.4rem;
        color: #3A3A3A;
        line-height: 2.4rem;

    }
     .algorithm  p span{
        display: inline-block;
        width: .4rem;
        height: 1.1rem;
        background-color: #9A9A9A;
     }
     .prompting {
        padding-top: 1.5rem;
        color: #A0A0A0;
        text-align: center;
        font-size: 1.2rem;
        line-height: 2.4rem;
     }
     .more{
        position: absolute;
        bottom: 0%;
        left: 0;
        width: 100%;
        text-align: center;
        line-height: 15rem;
         font-size: 1.2rem;
          color: #A0A0A0;
    // padding-top: 9rem;
    // width: 100%;
    // text-align: center;
    // line-height: 15rem;
    // font-size: 1.2rem;
    // color: #A0A0A0;

     }
     .loading{
        // display: flex;
        left: 0;
        top: 0;
        position: fixed;
        width: 100%;
        height: 100%;
        z-index: 1000;
        font-size: 10rem;
        display: flex;
        justify-content: center;
        // flex-wrap:wrap-reverse;
        
        background-color: rgba(0,0,0,.7)

     }
     .loading svg{
        width: 3rem;
        // height: 20rem;
        // a: red;
        // background-color: red;
     }
     .more img{
        width: 1.2rem;
        margin-right: .5rem;
     }
     .page1{
        position: fixed;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
     }
}

  

.slideInDown {
    animation-name: slideInDown;
     animation-duration: 0.8s;
    animation-fill-mode: both
}
.slideInUp {
    animation-name: slideInUp;
     animation-duration: 0.8s;
    animation-fill-mode: both
}

.slideOutDown {
    animation-name: slideOutDown;
     animation-duration: 1.6s;
    animation-fill-mode: both
}
.slideOutUp {
    animation-name: slideOutUp;
     animation-duration: 1.2s;
    animation-fill-mode: both
}


 @keyframes slideOutUp {
    0% {
        transform: translateZ(0)
    }

    to {
        visibility: hidden;
        transform: translate3d(0,-100%,0)
    }
}



@keyframes slideOutDown {
    0% {
        transform: translateZ(0)
    }

    to {
        visibility: hidden;
        transform: translate3d(0,100%,0)
    }
}




 @keyframes slideInUp {
    0% {
        transform: translate3d(0,100%,0);
      
    }

    to {
        transform: translateZ(0)
    }
}



@keyframes slideInDown {
    0% {
        transform: translate3d(0,-100%,0);
        
    }

    to {
        transform: translateZ(0)
    }
}



        background: #fff;
        height: 0.2rem;
        vertical-align: text-top;
    }
    .progress-info {
        width: 34.4rem;
        margin: 0 auto;
        padding-bottom: 0.6rem;
    }
    .progress-info div {
        font-size: 1.2rem;
        color: #fff;
        line-height: 1.7rem;
    }
    .calculator {
        background: #fff;
    }
    .calculator-content {
        margin-left: 1.7rem;
    }
    .border {
        border-top: 1px solid #E1E1E1;
    }
    .calculator-common {
        height: 4.5rem;
            margin-right: 1.5rem;
    }
    .calculator-common span,
    .calculator-common input {
        font-size: 1.6rem;
        line-height: 4.4rem;
    }
    .calculator-common input {
        margin-left: 1.5rem;
             text-align: right;
             // margin-right: 1.5rem;
        vertical-align: baseline;
         float: right;
        //          margin-left: 5rem;
        //          
    }
    .calculator-common .line {
        width: 0.1rem;
        height: 1.6rem;
        color: #2772FF;
        border: 0.05rem solid #2772FF;

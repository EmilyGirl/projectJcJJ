.product-detail {
     // .page1 
    .bang-mask {
        position: absolute;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5);
        z-index: 10000;
    }
    .bang-content {
        width: 32.7rem;
        height: 17rem;
        background: #fff;
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        border-radius: 1.25rem;
    }
    .bang-content p {
        text-align: center;
    }
    .bang-title {
        font-size: 1.8rem;
        margin: 1.5rem 0;
        color: #000;
    }
    .bang-common {
        font-size: 1.6rem;
        color: #666;
        line-height: 2.3rem;
        margin-bottom: 1.5rem;
    }
    .bangBtn {
        width: 90%;
        margin: 0 auto;
        
    }
    
    .bangBtn a,
    .bangBtn span {
        display: inline-block;
        width: 13.2rem;
        height: 4rem;
        text-align: center;
        line-height: 4rem;
        font-size: 1.6rem;
        color: #fff;
        border: 1px solid #2772FF;
        border-radius: 0.4rem;
    }
    /*.bangBtn a span { display: inline-block;width: 100%;height: 100%; }*/
    .bangBtn .firstBtn {
        color: #2772FF;
    }
    .secondBtn {
        background: #2772FF;
    }
    .product-header {
        background: #2772FF;
        height: 21.2rem;
        text-align: center;
        background-size: cover;
    }
    .product-header .title-top {
        font-size: 1.4rem;
        line-height: 2rem;
        color: #fff;
        text-align: center;
        // margin: 1rem 0; 
        padding-top: 1rem;
    }
    .title {
        text-align: left;
        padding: 1.6rem 0 0 1.5rem;
        font-size: 1.4rem;
        color: #fff;
        line-height: 2rem;
    }
    .interest {
        text-align: center;
    }
    .yuan {
        font-size: 5.3rem;
        line-height: 6rem;
        color: #fff;
    }
    .yuan-mum {
        font-size: 3rem;
        color: #fff;
    }
    .jiali {
        font-size: 3rem;
        color: #fff;
    }
    .jiali-num {
        font-size: 1.5rem;
        color: #fff;
    }
    .info-title {
        text-align: center;
        margin: 1rem 0;
    }
    .info-title span {
        display: inline-block;
        padding: 0.5rem 0.9rem;
        border: 1px solid #fff;
        border-radius: 0.4rem;
        font-size: 1.2rem;
        color: #fff;
        margin-left: 1rem;
    }
    .detail {
        font-size: 1.6rem;
        color: #fff;
        line-height: 2.3rem;
        padding-bottom: 3.5rem;
    }
    .progress {
        padding-top: 1.2rem;
        background: #2772FF;
        text-align: center;
        margin-bottom: 1.2rem;
    }
    .schedule {
        font-size: 1.1rem;
        color: #2772FF;
        text-align: left;
        margin-bottom: 0.9rem;
        display: inline-block;
    }
    .schedule1 {
        width: 34.4rem;
    }
    .schedule .progress-width {

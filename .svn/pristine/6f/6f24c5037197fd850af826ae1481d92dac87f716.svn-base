.product {
    .noData{
        text-align: center;
        font-size: 1.5rem;
        line-height: 6rem;
        img{
            margin-top: 40%;
            width: 60%;
        }
    }
	.product-lists li{
		padding: 1.3rem 2.7rem 1.5rem 2.4rem;
		background: #fff;
		margin-top: 1rem;
	}
	.product-lists li:first-child {
		margin: 0;
	}
	.product-date,
	.product-name {
		display: inline-block;
	}
	.product-name {
		font-size: 1.8rem;
		color: #555;
		line-height: 2.5rem;
	}
	.product-date {
		width: 5rem;
		height: 1.5rem;
		border: 1px solid #999;
		font-size: 0.9rem;
		color: #999;
		text-align: center;
		line-height: 1.5rem;
		border-radius: 1.5rem;
		margin-left: 2rem;
	}
	.middleLeft span {
		font-size: 3.2rem;
		color: #FE640E;
		line-height: 4.5rem;
	}
	.middleLeft span span {
		font-size: 1.6rem;
	}
	.middleLeft span var {
		font-size: 2.4rem;
	}
	.middleLeft span var span {
		font-size: 1.2rem;
	}
	.middleLeft p {
		font-size: 1.1rem;
		color: #666;
		line-height: 1.5rem;
	}
	.middleRight,
	.noMoney {
		width: 9.3rem;
		height: 2.3rem;
		background: #2772FF;
		border-radius: 0.4rem;
		color: #fff;
		font-size: 1.2rem;
		margin-top: 1.3rem;
	}
	.noMoney {
		background: #DBDBDB;
	}
	.schedule {
		font-size: 1.1rem;
		color: #2772FF;
		line-height: 1.3rem;
	}
	.schedule span {
		display: inline-block;
		height: 2px;
		width: 16.1rem;
		background: #eaebef;
		vertical-align: middle;
		margin-right: 1.2rem;
	}
	.schedule span var {
		display: inline-block;
		background: #2772FF;
		height: 2px;
		vertical-align: text-top;
	}
	/*home*/
	.tab {
		width: 100%;
		padding: 1.03rem 0 0.78rem 0;
		background: #fff;
		position: absolute;
		font-size: 1.2rem;
		z-index: 1000;
		bottom: 0;
		border-top: .1rem solid #E1E1E1;
	}
	.tab li {
		width: 50%;
		float: left;
		font-size: .9em;
		text-align: center;
	}
	.tab li a {
		color: #999;
	}
	.tab li.cur a {
		color: #333;
	}
	.tab .active.nav-item:nth-child(1) span {
		width: 1.845rem;
		height: 1.74rem;
		display: block;
		background: url(../../img/wechat/shouye/cmoney.png) no-repeat;
		background-size: 100% 100%;
		margin: 0 auto .375rem auto;
	}
	.tab .nav-item:nth-child(1) span {
		width: 1.845rem;
		height: 1.74rem;
		display: block;
		background: url(../../img/wechat/shouye/kmoney.png) no-repeat;
		background-size: 100% 100%;
		margin: 0 auto .375rem auto;
	}
	.tab .active.nav-item:nth-child(2) span {
		width: 1.845rem;
		height: 1.74rem;
		display: block;
		background: url(../../img/wechat/shouye/cmine.png) no-repeat;
		background-size: 100% 100%;
		margin: 0 auto .375rem auto;
	}
	.tab .nav-item:nth-child(2) span {
		width: 1.845rem;
		height: 1.74rem;
		display: block;
		background: url(../../img/wechat/shouye/mine.png)/* -50px -5px / 200px 100px*/
		no-repeat;
		background-size: 100% 100%;
		margin: 0 auto .375rem auto;
	}
	/*.nav-item*/
	.tab .active a {
		color: #2772FF;
	}
	/*.tab .active a span {
    background-position-y: -55px;
}*/
	.wapper {
		width: 100%;
		height: 100%;
		background: #eaeaea;
		position: absolute;
	}
	.clearfix::after {
		content: "";
		display: block;
		height: 0;
		clear: both;
		visibility: hidden
	}
}
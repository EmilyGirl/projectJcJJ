/*注册*/
	
	.revise_traders_pwd .input-list {
		background: #fff;
		margin-top: 1.1rem;
	}
	
	.revise_traders_pwd .icommon {
		border-bottom: 1px solid #e5e5e5;
		margin-left: 2.5rem;
	}
	
	.revise_traders_pwd .icommon:last-child {
		border: 0;
	}
	
	.revise_traders_pwd .icommon label {
		font-size: 1.5rem;
		display: inline-block;
		width: 6.5rem;
		-webkit-user-select: none;
		vertical-align: middle;
		margin-right: 1.6rem;
	}
	.revise_traders_pwd span{
		font-size: 1.5rem;
		vertical-align: middle;
	}
	.revise_traders_pwd .icommon.bank{
		padding: 1.75rem 0;
	}
	.revise_traders_pwd .icommon .bankImg{
		width: 5.5rem;
		height: 5.5rem;
		border-radius: 2.75rem;
		overflow: hidden;
		-webkit-user-select: none;
	}
	.revise_traders_pwd .bankImg img{
		height: 100%;
		-webkit-user-select: none;
	}
	
	.revise_traders_pwd .icommon .hide,
	.revise_traders_pwd .icommon .show {
		margin: 0 0 0 6.5rem;
	}
	
	.revise_traders_pwd .icommon .show {
		margin-left: 3.5rem;
	}
	
	.revise_traders_pwd .icommon input {
		font-size: 1.4rem;
		padding: 1.5rem 0;
		width: 17rem;
	}
	
	.revise_traders_pwd .icommon .password {
		width: 17rem;
	}
	
	.revise_traders_pwd .icommon .recharge_input{
		width: 24rem;
	}
	
	.revise_traders_pwd .fr {
		float: inherit;
		font-size: 1.4rem;
		vertical-align: middle;
		color: #2772FF;
	}
	
	.revise_traders_pwd .getMa {
		width: 9rem;
		height: 3rem;
		border-radius: .5rem;
		border: 0;
		outline: none;
		color: #fff;
		font-size: 1.5rem;
		background: #2772FF;
		vertical-align: middle;
	}
	
	.revise_traders_pwd .normal{
		color: #999999;
		margin: .9rem 0 0 2.5rem;
		font-size: 1.2rem;
	}
	
	.revise_traders_pwd .bkTitle{
		font-size: 1.5rem;
		color: #000000;
		padding: 1.5rem 0 0 0;
	}
	
	.revise_traders_pwd .light{
		color: #2772FF;
	}
	
	.revise_traders_pwd .tips{
		color:red;
	}
	
	.revise_traders_pwd .boolBtn{
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		color: #999;
	}
	
	.revise_traders_pwd .registerBtn,
	.revise_traders_pwd .loginBtn {
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		color: #fff;
		background: #2772FF;
	}
	
	.revise_traders_pwd .registerBtn:active,
	.revise_traders_pwd .loginBtn:active {
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		color: #fff;
		background: #555555;
	}
	
	.revise_traders_pwd .loginBtn {
		opacity: 1;
	}
	
	.revise_traders_pwd .ser-res {
		text-align: center;
		font-size: 1.2rem;
		color: #ccc;
	}
	
	.revise_traders_pwd .ser-res a {
		color: #629DFC;
	}
	
	.revise_traders_pwd .login-bottom {
		width: 92%;
		margin: 0 auto;
	}
	
	.revise_traders_pwd .login-bottom p {
		color: #FB5C04;
		font-size: 1.2rem;
	}
	/*渐变*/
	
	.revise_traders_pwd .welfare-close {
		height: 4rem;
		background: rgba(0, 0, 0, 0.6);
		font-size: 1.5rem;
		color: #fff;
		text-align: center;
		line-height: 4rem;
		border-radius: 2rem;
		position: fixed;
		top: 50%;
		left: 50%;
		transform: translate(-50%, -50%);
		-webkit-transform: translate(-50%, -50%);
		-moz-transform: translate(-50%, -50%);
		-o-transform: translate(-50%, -50%);
		padding: 0 1rem;
		z-index: 100000;
	}
	
	.revise_traders_pwd .fade-enter-active,
	.revise_traders_pwd .fade-leave-active {
		transition: opacity .3s;
	}
	
	.revise_traders_pwd .fade-enter,
	.revise_traders_pwd .fade-leave-to
	/* .fade-leave-active in below version 2.1.8 */
	
	{
		opacity: 0
	}
	


/*wx_recharge*/
.revise_traders_pwd .bankText{
	display: inline-block;
	font-size: 1.3rem;
	vertical-align: middle;
	color: #999999;
}
.revise_traders_pwd .bankText p:first-of-type{
	font-size: 1.6rem;
	color: #000000;
	margin-bottom: 2rem;
}

	.wx_recharge .boolBtn{
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		background: #ccc;
	
		
	}
	
	.wx_recharge .registerBtn,
	.wx_recharge .loginBtn {
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		color: #fff;
		background: #2772FF;
	}
	
	.wx_recharge .registerBtn:active,
	.wx_recharge .loginBtn:active {
		width: 92%;
		height: 4.5rem;
		display: block;
		margin: 2rem auto 1.5rem;
		border-radius: 0.4rem;
		font-size: 1.6rem;
		color: #fff;
		background: #555555;
	}
	
	.wx_recharge .loginBtn {
		opacity: 1;
	}
	
	.wx_recharge .ser-res {

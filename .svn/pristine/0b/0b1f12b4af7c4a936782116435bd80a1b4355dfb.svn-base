.withdraw *{
	-webkit-appearance:none !important;
    -moz-appearance: none !important;
    appearance: none !important;

.withdraw .normal {
	color: #999999;
	font-size: 1.2rem;
	margin-top: 1.2rem;
}

.withdraw .normal .input-list {
	background: #fff;
	padding: 0 1.8rem;
}
.withdraw .normal .first {
	background: #fff;
	padding: 0 1.8rem;
	display: flex;
	justify-content: space-between;
	flex-wrap: nowrap;
}

.withdraw .bkTitle {
	font-size: 1.5rem;
	color: #000000;
	margin: 2.8rem 0 2.4rem 0;
}

.withdraw .icommon {
	padding: 1.4rem 0;
}
.withdraw .icommon_input{
	padding: 2.3rem 0;
	white-space: nowrap;
}
.withdraw .normal .left {
	padding-left: 1.8rem;
	margin: 2rem 0;
}

.withdraw .light {
	
	color: @mainColor;
}

.withdraw .icommon_input label {
	font-size: 1.5rem;
	color: #000;
	
}

.withdraw .icommon_input input {
	vertical-align: baseline;
	font-size: 1.5rem;
	margin-left: 1.5rem;
	width: 50%;
}

.withdraw .icommon_input .all {
	font-size: 1.5rem;
	color: #2772FF;
	margin: 2.3rem 0 2.3rem 2.5rem;
}

.withdraw .tips {
	color: red;
}

.withdraw .boolBtn {
	width: 92%;
	height: 4.5rem;
	display: block;
	margin: 2rem auto 1.5rem;
	border-radius: 0.4rem;
	font-size: 1.6rem;
	background: #CCCCCC;

	
}

.withdraw .registerBtn,
.withdraw .loginBtn {
	width: 92%;
	height: 4.5rem;
	display: block;
	margin: 2rem auto 1.5rem;
	border-radius: 0.4rem;
	font-size: 1.6rem;
	color: #fff;
	background: #2772FF;
}

.withdraw .registerBtn:active,
.withdraw .loginBtn:active {
	width: 92%;
	height: 4.5rem;
	display: block;
	margin: 2rem auto 1.5rem;
	border-radius: 0.4rem;
	font-size: 1.6rem;
	color: #fff;
	background: #555555;
}

.withdraw .loginBtn {
	opacity: 1;
}

.withdraw .ser-res {
	text-align: center;
	font-size: 1.2rem;
	color: #ccc;
}

.withdraw .ser-res a {
	color: #629DFC;
}

.withdraw .login-bottom {
	width: 92%;
	margin: 0 auto;
}

.withdraw .login-bottom p {
	color: #FB5C04;
	font-size: 1.2rem;
}


/*渐变*/

.withdraw .welfare-close {
	/*height: 4rem;*/
	background: rgba(0, 0, 0, 0.6);
	font-size: 1.5rem;
	color: #fff;
	text-align: center;
	line-height: 3rem;
	border-radius: 2.2rem;
	position: fixed;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	padding: 0.8rem 1rem;
	z-index: 100000;
}

.withdraw .fade-enter-active,
.withdraw .fade-leave-active {
	transition: opacity .3s;
}

.withdraw .fade-enter,
.withdraw .fade-leave-to
/* .fade-leave-active in below version 2.1.8 */

{
	opacity: 0
}


/*withdraw*/

.withdraw .bankText {
	display: inline-block;
	font-size: 1.3rem;
	vertical-align: middle;
	color: #999999;
}

.withdraw .bankText .bankImg {
	display: inline-block;
}

.withdraw .bankText p:first-of-type {
	display: inline-block;
	font-size: 1.6rem;
	color: #000000;
	margin-bottom: 2rem;
}
.withdraw .footTitle {
	margin-top:0.9rem;
	margin-left: 1.8rem;
}
.withdraw .footTitle p{font-size: 1.2rem;color:#999999; margin-bottom: 1.2rem;}
.withdraw .reminder{
	margin: 16.7rem auto 2rem;
	text-align:center;
	font-size: 1.2rem;
	color: #999999;
}
 .withdraw .reminder .reminder_phone{
 	
 	margin:0.8rem 0;
 	color: #2772FF;
 	font-size: 1.8rem;

 }
  .withdraw .reminder .reminder_phone .reminder_phoneico{
  	    display: inline-block;
  	width: 2rem;
  	height:2rem;
  	background:url('../../img/wechat/mine/reminder_phone.png')no-repeat;
  	background-size:100%;
  	    margin-right: 1.2rem;
  	    vertical-align: text-top;
  }
}
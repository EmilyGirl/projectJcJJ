
.zeek{
	*{
    margin: 0;
    padding: 0;
    box-sizing: border-box
}
    overflow: hidden;
    /* z-index: 1; */

.clear{
    clear: both;
}
.wrapped_title{
    width: 100%;
    height: 4.5rem;
    background: #FFFFFF;
    border-top: 1px solid #EBEBEB;
    border-bottom:1px solid #EBEBEB;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 11;
}
.wrapped_box{
    margin-top: 4.5rem;
    /* z-index: 11; */
}
.wrapped_title li{
    float: left;
    width: 20%;
    margin: 0 2.2rem;
    height: 4.5rem;
    text-align:center;
    line-height: 4.5rem;
    font-size: 1.5rem;
    color: #000000;
}
.wrap_li{
    color: #2772FF!important;
    border-bottom: 0.2rem solid #2772FF;
}
.zeek_rewards{
    width: 37.5rem;
    height: auto;
    // margin: 1rem 0;
    /* padding: 0.8rem 0 0 0; */
    // background: #fff;
    /* z-index: 111; */
}

.clear{
    clear: both;
}

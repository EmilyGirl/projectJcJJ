 table{
     width: 100%;
     margin: 0 auto;
     margin-top: 5rem;
 }
thead td{
    font-size: 1.2rem;
color: #A0A0A0;
text-align: center;
width: 33%;
height: 4.8rem;
line-height: 4.8rem;
}
tbody tr td{
    height: 4.8rem;
   text-align: center;
    line-height: 4.8rem;
    font-size: 1.4rem;
color: #606060;
}
tbody tr  td:nth-of-type(1){
    width: 20%;
}
tbody tr  td:nth-of-type(2){
    width: 40%;
}
tbody tr  td:nth-of-type(3){
    width: 20%;
}
tbody tr:nth-child(odd){
    background: #fff;
}

